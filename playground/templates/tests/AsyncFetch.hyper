# Async data fetching - triggers async def generation
user_id: int
fetch_user: Callable[[int], Awaitable[dict]]
fetch_posts: Callable[[int], Awaitable[list]]

# await keyword triggers async def __hyper_template__
user = await fetch_user(user_id)
posts = await fetch_posts(user_id)

<div class="async-content">
    <h1>{user['name']}</h1>

    <div class="posts">
        for post in posts:
            <article>
                <h2>{post['title']}</h2>
                <p>{post['content']}</p>
            </article>
        end
    </div>
</div>

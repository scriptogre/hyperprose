# Card with default parameter values
title: str = "Untitled"
content: str = ""
variant: str = "default"
show_border: bool = True

<div class="card card-{variant} {'bordered' if show_border else ''}">
    <h2>{title}</h2>
    if content:
        <p>{content}</p>
    else:
        <p class="empty">No content provided</p>
    end
</div>

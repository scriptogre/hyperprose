# Various HTML attribute patterns
id: str
classes: list[str]
data: dict
disabled: bool = False
url: str = "#"

<div
    id="{id}"
    class="component {' '.join(classes)}"
    data-id="{data.get('id', '')}"
    data-config='{{"key": "value"}}'
    aria-label="Component label"
    role="region">

    <a href="{url}" target="_blank" rel="noopener noreferrer">
        Link
    </a>

    <button
        type="button"
        class="btn {'disabled' if disabled else ''}"
        disabled="{disabled}"
        onclick="handleClick()">
        Click me
    </button>

    <input
        type="text"
        name="field"
        value="{data.get('value', '')}"
        placeholder="Enter value..."
        required/>
</div>

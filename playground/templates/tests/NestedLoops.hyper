# Nested loop patterns - tables, grids
rows: list[list]
headers: list[str]

<table class="data-table">
    <thead>
        <tr>
            for header in headers:
                <th>{header}</th>
            end
        </tr>
    </thead>
    <tbody>
        for row in rows:
            <tr>
                for cell in row:
                    <td>{cell}</td>
                end
            </tr>
        end
    </tbody>
</table>

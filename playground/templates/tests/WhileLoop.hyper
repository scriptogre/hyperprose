# While loop patterns
max_iterations: int = 10

counter = 0
<div class="while-demo">
    while counter < max_iterations:
        <span class="iteration">{counter}</span>
        counter = counter + 1
    end
</div>

# While with break condition in body
items = [1, 2, 3, 4, 5]
i = 0
<ul>
    while i < len(items):
        <li>{items[i]}</li>
        i += 1
    end
</ul>
